<!--pages/trends/trends.wxml-->
<import src="/pages/template/ariticles/ariticles.wxml"/>
<!--使用template时要记得引用-->
<swiper autoplay indicator-dots indicator-active-color="black" interval="5000" circular="ture" class="swiper"> 
<swiper-item>
<image src="https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1562565358177&di=c8dbcf3fac000f198fc64b0000b24b8e&imgtype=0&src=http%3A%2F%2Fimg.pconline.com.cn%2Fimages%2Fupload%2Fupc%2Ftx%2Fphotoblog%2F1602%2F22%2Fc20%2F18513740_1456152019573.jpg" class="swiper_picture" mode="widthFix"></image>
</swiper-item>
<swiper-item>
<image src="data:image/jpeg;base64," class="swiper_picture" mode="widthFix"></image>
</swiper-item>
<swiper-item>
<image src="https://en.xidian.edu.cn/__local/4/40/59/48895B61AFC925240BC445A5BF0_957F44FB_27D23.jpg" class="swiper_picture" mode="widthFix"></image>
</swiper-item>
</swiper>

<view 
wx:for="{{listArray}}" 
wx:key="{{index}}" 
bindtap="goto" 
data-id="{{listArray[index].movie_id}}"
data-type="1" >
    <template is="ariticles" data="{{...item}}"/>
</view>
<!--currentTarget和Target中的dataset是标签中的元素-->